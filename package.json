		"tailwindcss": "^3.4.17",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.19.0",
		"vite": "^6.0.7",
		"vitest": "^2.1.8",
		"web-bip39": "^0.0.3"
	},
	"overrides": {
